setxkbmap br &
xset +fp /usr/share/fonts/misc
xset +fp ~/.fonts
xset fp rehash
xset r rate 300 50 &
xrdb "${XDG_CONFIG_HOME}"/X11/xresources # Before the bars so they get correct colors.
mpd &
sxhkd &
syncthing -no-browser &
volume-output | xob &
gpgconf --launch gpg-agent &
xbanish &
wm &
rndw &
notify-new-mail &
wpa_cli -a $HOME/bin/notify-wifi &
start-pulseaudio-x11 &
xidle -timeout 900 -program /usr/bin/slock &
sudo sh -c 'echo 100 > /sys/class/backlight/intel_backlight/brightness'
export DISPLAY=:1 && exec dbus-run-session xwait
